	"latex-workshop.view.pdf.viewer": "external",
	"latex-workshop.view.pdf.ref.viewer":"external",
	"latex-workshop.view.pdf.external.viewer.command": "D:/APPs&Tools/SumatraPDF/SumatraPDF.exe",
	"latex-workshop.view.pdf.external.viewer.args": [
		"-inverse-search",
		"\"D:/APPs&Tools/Microsoft VS Code/Code.exe\" \"D:/APPs&Tools/Microsoft VS Code/resources/app/out/cli.js\" -r -g \"%f:%l\"",
		"%PDF%"
		],
	"latex-workshop.view.pdf.external.synctex.command": "D:/APPs&Tools/SumatraPDF/SumatraPDF.exe",
	"latex-workshop.view.pdf.external.synctex.args":[
		"-forward-search",
		"%TEX%",
		"%LINE%",
		"%PDF%",
	],
